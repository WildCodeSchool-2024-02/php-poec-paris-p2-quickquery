<!doctype html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="/assets/images/favicon.png">
    <link rel="bootstrap/css" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css">
    <link rel="bootstrap/js" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js">


    <title>{% block title %}{% endblock %}</title>

    <link rel="stylesheet" href="/assets/css/style.css">


    
    {% block stylesheet %}{% endblock %}
</head>

<body>
    <nav class="navbar navbar-expand-lg bg-light" data-bs-theme="light">
  <div class="container-fluid">
    <img src="/assets/images/quickquery_logo_3.png" id="logo" class="d-inline-block align-top" alt="logo">
    <button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor03" aria-controls="navbarColor03" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="navbar-collapse collapse" id="navbarColor03" style="">
      <ul class="navbar-nav me-auto">
      <form class="d-flex col-xs-4">
        <input class="form-control me-md-3" type="search" placeholder="Search for a query">
        <button class="btn btn-dark me-md-5" type="submit">Search</button>
      </form>
      
        <li>
       <button type="button" class="btn btn-primary">POSER UNE QUESTION</button>
        </li>
        </li>
      </ul>
    </div>
  </div>
</nav>
    {% block content %}{% endblock %}
    {% block javascript %}{% endblock %}
</body>
</html>


